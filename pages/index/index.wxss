/* 医药服务 */
/* pages/med/index.wxss */
.med_server{
  width: 100%;
  /* height: 100vh; */
  background-color: #f8fafc;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  padding-bottom: 100rpx;
  padding-top: 15rpx;
  /* background-image: url(https://img-blog.csdnimg.cn/20200620110312470.png); */
}
.med_server .con_item{
  width: 85%;
  height: 220rpx;
  background-color: #fff;
  margin:15rpx 0;
  border: 20rpx;
  border-radius: 20rpx;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  padding: 20rpx;
  display: flex;
}
.med_server .con_item .con_item_left{
  width: 60%;
  height: 100%;
  padding-left: 20rpx;
}
.med_server .con_item .con_item_left .left_title{
  width: 100%;
  height: 60%;
  display: flex;
  flex-direction: column;
}
.med_server .con_item .con_item_left .left_title .title{
  font-size: 36rpx;
  font-weight: bold;
  letter-spacing: 5rpx;
  line-height: 2;
}
.med_server .con_item .con_item_left .left_title .sub_title{
  font-size: 26rpx;
  letter-spacing: 5rpx;
  color: #ccc;
}
.med_server .con_item .con_item_left .left_see{
  width: 100%;
  height: 40%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.med_server .con_item .con_item_left .left_see .see_btn{
  width: 40%;
  height: 50%;
  font-size: 30rpx;
  color: #99CCFF;
  border-radius: 50rpx;
  border:1px solid #99CCFF;
  line-height: 1.5;
  text-align: center;
}
.med_server .con_item .con_item_right{
  width: 40%;
  height: 100%;
  /* background-color: red; */
  display: flex;
  justify-content: center;
  align-items: center;
}
.med_server .con_item .con_item_right image{
  width: 300rpx;
  height: 250rpx;
}
.med_server .con_item .con_item_right_special image{
  width: 180rpx;
  height: 190rpx;
}
.med_server .background_title{
  width: 100%;
  height: 20vh;
  background-color: #336699;
  position: fixed;
  top: 0;
  z-index: -1;
}


