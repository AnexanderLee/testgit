/* 首页 */

.index_map{
  height: 62vh;
}
.display{
  width: 100%;
  position: fixed;
  background-color: #fff;
  bottom: 250rpx;
  border-radius: 20px 20px 0 0 ;
  box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04)
}
.display_title{
  height: 80px;
  border-radius: 20px 20px 0 0 ;
  border-bottom: 1px solid #eee;
}
.display_title .line{
  height: 20%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.display_title .line .line_cont{
  width: 60rpx;
  height: 10rpx;
  background-color: #eee;
  border-radius: 20rpx;
}
.display_title .title_main{
  height: 80%;
  display: flex;
}
.display .title_main .main_left{
  width: 80%;
  height: 100%;
}
.display .title_main .main_right{
  width: 20%;
  height: 100%;
  display: flex;
  /* flex-direction: column; */
  /* justify-content: space-around; */
}
.display .title_main .main_left .location{
  width: 100%;
  height: 35%;
  display: flex;
  align-items: center;
}
.display .title_main .main_left .location .location_icon{
  color: #ccc;
  margin-left: 20rpx;
}
.display .title_main .main_left .location .location_text{
  color: #ccc;
  margin-left: 20rpx;
  font-size: 20rpx;
}
.display .title_main .main_left .message{
  width: 100%;
  height: 65%;
  display: flex;
  align-items: center;
}
.display .title_main .main_left .message .mes_con{
  font-size: 42rpx;
  margin-left: 20rpx;
  color: #505050;
}
.display_list .scroll_left .scroll_right{
 width: 100%;
 /* height: 200px; */
}
.display_list .scroll_left .warning_icon{
  color: #db3131;
  font-size: 36rpx;
  font-weight: bold;
  margin-right: 18rpx;
}
.display_list .scroll_right .custom-icon{
  font-size: 36rpx;
  color: #db3131;
}
