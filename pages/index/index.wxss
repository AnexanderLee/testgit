/* 同乘查询 */
.stand_search{
  width: 100%;
}
.search_tip{
  height: 25vh;
  background-color:#336699;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.search_tip .tip_title{
  width: 80%;
  display: flex;
  justify-content: center;
}
.search_tip .tip_title .title_left{
  display: block;
  width: 40%;
  height: 65rpx;
  background-color: red;
  font-size: 48rpx;
  font-weight: 800;
  color: #fff;
  text-align: center;
  border-radius: 20rpx 0 20rpx 0;
}
.search_tip .tip_title .title_right{
  display: block;
  margin-left: 10rpx;
  width: 60%;
  height: 65rpx;
  font-size: 48rpx;
  font-weight: 800;
  color: #fff;
}
.search_tip .tip_con{
  width: 90%;
  height: 160rpx;
  border-radius: 20rpx;
  background-color: rgba(255, 255, 255, .1);
  margin-top: 20rpx;
  padding:14rpx;
  font-size: 28rpx;
  color: #99CCCC;
  text-indent: 2em;
  
}
.search_form{
  background-color: #fff;
  height: 55vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.search_form .form_main{
  width: 85%;
  height: 85%;
  border-radius: 20rpx;
  padding: 30rpx;
  box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04)
}
.search_form .form_main .form_title{
  height: 15%;
  border-bottom: 1px solid #eee;
  font-size: 36rpx;
  font-weight: bold;
  color: #3d4247;
  line-height: 2;
  letter-spacing: 3rpx;

}

.search_form .form_main .form_out_type{ 
  width: 100%;
  height: 22%;
  border-bottom: 1px solid #eee;
  display: flex;
}
.search_form .form_main .form_out_type .out_type_left{
  width: 80%;
  height: 100%;
}
.search_form .form_main .form_out_type .out_type_left .type_left_top{
  height: 20%;
  font-size: 28rpx;
  line-height: 2;
}
.search_form .form_main .form_out_type .out_type_left .type_left_bottom{
  height: 80%;
  font-size: 36rpx;
  line-height: 3;
  color: #ccc;

}
.search_form .form_main .form_out_type .out_type_right{
  width: 20%;
  height: 100%;
  font-size: 36rpx;
  line-height: 4;
  color: #3e6595;
}
.search_form .form_main .form_name { 
  width: 100%;
